  },
  "packageManager": "pnpm@10.23.0",
  "dependencies": {
    "axios": "^1.13.2"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vite-tsconfig-paths": "^5.1.4"
